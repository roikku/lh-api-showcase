<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Showcase'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User' />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard' />-->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     --> 

  <!-- Other module inherits                                      -->
  <inherits name="com.google.gwt.logging.Logging"/>
  <!-- <inherits name='com.google.gwt.visualization.Visualization'/>-->
  <inherits name="com.google.gwt.i18n.I18N"/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='lh.api.showcase.client.Showcase' />

  <!-- For production, most teams prefer to set the default log level to `OFF` -->
  <inherits name="com.allen_sauer.gwt.log.gwt-log-OFF" />
  <!-- For development, a default of `DEBUG` is recommended -->
  <!-- <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" /> -->

  <!--<inherits name='com.googlecode.mgwt.MGWT'/>-->

  <!--  Use Vaadin widgets -->
  <inherits name='com.vaadin.Vaadin'/>
  
	<!-- Use the custom theme -->
  <!--<inherits name='lh.api.showcase.ValoTheme' />-->
  
  <!-- Inherit to use the default Valo theme -->
  <inherits name='com.vaadin.themes.Valo'/>

  <!-- Provide the same HTML/JS to all browsers -->
  <collapse-all-properties/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client' />
  <source path='shared' />

</module>
